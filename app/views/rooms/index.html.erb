<main>
  <h3>ルーム一覧</h3>

   <table>
    <thead>
      <tr>
        <th>ルーム画像</th>
        <th>ID</th>
        <th>ルーム名</th>
        <th>ルーム紹介</th>
        <th>料金</th>
        <th>ユーザーid</th>
        <th>住所</th>
      </tr>
    </thead>
    <tbody>
      <% @rooms.each do |room| %>
        <tr>
          <td><%= room.room_image %></td>
          <td><%= room.id %></td>
          <td><%= room.room_name %></td>
          <td><%= room.room_introduction %></td>
          <td><%= room.room_rate %></td>
          <td><%= room.user_id %></td>
          <td><%= room.room_address %></td>


        </tr>
      <% end %>
    </tbody>
  </table>

</main>
